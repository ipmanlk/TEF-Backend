[
    {
        "regex": "^[\\w\\s]{5,200}$",
        "attribute": "name",
        "error": "Please provide a valid product package name!."
    },
    {
        "regex": "^.+(jpg|png|gif)$",
        "attribute": "photo",
        "error": "Please select a valid picture!.",
        "base64": true
    },
    {
        "regex": "^[\\d]+$",
        "attribute": "productId",
        "error": "Please provide a valid product id!."
    },
    {
        "regex": "^[\\d]{1,9}\\.[\\d]{2}$",
        "attribute": "price",
        "error": "Please provide a valid price!."
    },
    {
        "regex": "^[\\d]{1,9}\\.[\\d]{2}$",
        "attribute": "salePrice",
        "error": "Please provide a valid sale price!."
    },
    {
        "regex": "^[\\d]+$",
        "attribute": "pieces",
        "error": "Please provide a valid number of pieces!."
    },
    {
        "regex": "^[\\d]+$",
        "attribute": "rop",
        "error": "Please provide a valid re-order point!."
    },
    {
        "regex": "^[\\d]{1,7}\\.[\\d]{2,3}$",
        "attribute": "weight",
        "error": "Please provide a valid weight!."
    },
    {
        "regex": "^[\\d]+$",
        "attribute": "unitTypeId",
        "error": "Please select a valid unit type for weight!."
    },
    {
        "regex": "^[\\d]+$",
        "attribute": "productPackageStatusId",
        "error": "Please select a valid product package status!."
    },
    {
        "regex": "^[\\d]+$",
        "attribute": "productPackageTypeId",
        "error": "Please select a valid product package type!."
    },
    {
        "regex": "^[\\w\\s\\d\\,\\\\\\.\\n\\/]{10,100}$",
        "attribute": "description",
        "error": "Please provide a valid description!.",
        "optional": true
    },
    {
        "regex": "^(\\d{4})-(\\d{2})-(\\d{2})$",
        "attribute": "addedDate",
        "error": "Please provide a valid added date!."
    }
]